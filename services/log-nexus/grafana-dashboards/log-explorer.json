{
  "__inputs": [
    {
      "name": "DS_LOKI",
      "label": "Loki",
      "description": "Loki datasource for log queries",
      "type": "datasource",
      "pluginId": "loki",
      "pluginName": "Loki"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "loki", "name": "Loki", "version": "1.0.0" },
    { "type": "panel", "id": "barchart", "name": "Bar chart", "version": "" },
    { "type": "panel", "id": "table", "name": "Table", "version": "" },
    { "type": "panel", "id": "logs", "name": "Logs", "version": "" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "description": "Omni Quantum Elite — Centralized Log Explorer with trace correlation, error analytics, and full-text search",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [
    {
      "asDropdown": false,
      "icon": "external link",
      "includeVars": true,
      "keepTime": true,
      "tags": ["omni-quantum"],
      "targetBlank": true,
      "title": "AI Observability",
      "type": "dashboards"
    }
  ],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "Overview",
      "type": "row"
    },
    {
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "description": "Total log lines ingested in the selected time range",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 100000 },
              { "color": "red", "value": 1000000 }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "id": 1,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false }
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
          "expr": "sum(count_over_time({omni_tier=~\".+\"} [$__interval]))",
          "legendFormat": "Total Lines",
          "refId": "A"
        }
      ],
      "title": "Total Log Lines",
      "type": "stat"
    },
    {
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "description": "Total error log lines in the selected time range",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 1000 }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "id": 2,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false }
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
          "expr": "sum(count_over_time({omni_tier=~\".+\"} |= \"ERROR\" [$__interval]))",
          "legendFormat": "Errors",
          "refId": "A"
        }
      ],
      "title": "Total Errors",
      "type": "stat"
    },
    {
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "description": "Total warning log lines",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 500 },
              { "color": "orange", "value": 5000 }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "id": 3,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false }
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
          "expr": "sum(count_over_time({omni_tier=~\".+\"} |= \"WARN\" [$__interval]))",
          "legendFormat": "Warnings",
          "refId": "A"
        }
      ],
      "title": "Total Warnings",
      "type": "stat"
    },
    {
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "description": "Number of active services logging",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 20 },
              { "color": "green", "value": 35 }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "id": 4,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
          "expr": "count(count_over_time({omni_tier=~\".+\"} [$__interval])) by (container)",
          "legendFormat": "Services",
          "refId": "A",
          "instant": true
        }
      ],
      "title": "Active Services",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 101,
      "title": "Log Volume & Error Analysis",
      "type": "row"
    },
    {
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "description": "Log volume over time, stacked by log level. Quickly identify error spikes and abnormal log patterns.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "Lines / sec",
            "fillOpacity": 80,
            "gradientMode": "none",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "lineWidth": 1,
            "stacking": { "group": "A", "mode": "normal" }
          },
          "unit": "ops"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "error" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "warning" },
            "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "info" },
            "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "debug" },
            "properties": [{ "id": "color", "value": { "fixedColor": "gray", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "critical" },
            "properties": [{ "id": "color", "value": { "fixedColor": "dark-red", "mode": "fixed" } }]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 6 },
      "id": 10,
      "options": {
        "barRadius": 0,
        "barWidth": 0.9,
        "groupWidth": 0.7,
        "legend": { "calcs": ["sum"], "displayMode": "table", "placement": "right" },
        "orientation": "auto",
        "showValue": "never",
        "stacking": "normal",
        "tooltip": { "mode": "multi", "sort": "desc" },
        "xTickLabelRotation": 0
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
          "expr": "sum by (level) (rate({omni_component=~\"$service\"} | json [$__interval]))",
          "legendFormat": "{{level}}",
          "refId": "A"
        }
      ],
      "title": "Log Volume by Level",
      "type": "barchart"
    },
    {
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "description": "Error count per service, sorted descending. Identify the noisiest error sources.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "auto" },
            "inspect": false
          },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 100 }
            ]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Error Count" },
            "properties": [
              { "id": "custom.cellOptions", "value": { "mode": "gradient", "type": "gauge" } },
              { "id": "color", "value": { "mode": "continuous-reds" } }
            ]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
      "id": 20,
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": true },
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "Error Count" }]
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
          "expr": "sum by (container) (count_over_time({omni_tier=~\".+\", level=\"error\"} [$__range]))",
          "legendFormat": "{{container}}",
          "refId": "A",
          "instant": true
        }
      ],
      "title": "Error Rate by Service",
      "transformations": [
        {
          "id": "reduce",
          "options": { "reducers": ["sum"] }
        },
        {
          "id": "sortBy",
          "options": { "fields": {}, "sort": [{ "field": "Total", "desc": true }] }
        },
        {
          "id": "organize",
          "options": {
            "renameByName": { "Field": "Service", "Total": "Error Count" }
          }
        }
      ],
      "type": "table"
    },
    {
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "description": "Top error messages aggregated by message text with count and last occurrence.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "auto" },
            "inspect": true,
            "minWidth": 200
          },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 5 }
            ]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Count" },
            "properties": [
              { "id": "custom.width", "value": 100 },
              { "id": "custom.cellOptions", "value": { "mode": "gradient", "type": "gauge" } },
              { "id": "color", "value": { "mode": "continuous-reds" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Service" },
            "properties": [{ "id": "custom.width", "value": 200 }]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
      "id": 50,
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": true, "fields": "", "reducer": ["sum"], "show": true },
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "Count" }]
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
          "expr": "topk(25, sum by (container, level) (count_over_time({omni_tier=~\".+\", level=\"error\", omni_component=~\"$service\"} [$__range])))",
          "legendFormat": "{{container}}",
          "refId": "A",
          "instant": true
        }
      ],
      "title": "Top Error Messages",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "renameByName": {
              "Field": "Error Message",
              "container": "Service",
              "Value": "Count"
            }
          }
        }
      ],
      "type": "table"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 22 },
      "id": 102,
      "title": "Log Stream",
      "type": "row"
    },
    {
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "description": "Live log stream, filterable by service, level, and free-text search. Shows full log entries with metadata.",
      "gridPos": { "h": 12, "w": 24, "x": 0, "y": 23 },
      "id": 30,
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": true,
        "showCommonLabels": false,
        "showLabels": true,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": true
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
          "expr": "{omni_component=~\"$service\", level=~\"$level\"} $search_filter",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Log Stream",
      "type": "logs"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 35 },
      "id": 103,
      "title": "Trace Correlation",
      "type": "row"
    },
    {
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "description": "Logs filtered by trace_id for end-to-end request tracing. Enter a trace ID from Langfuse to see all related logs.",
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 36 },
      "id": 40,
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": true,
        "showCommonLabels": false,
        "showLabels": true,
        "showTime": true,
        "sortOrder": "Ascending",
        "wrapLogMessage": true
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
          "expr": "{omni_tier=~\".+\"} | json | trace_id = `$trace_id`",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Trace Correlation — Logs for Trace ID",
      "type": "logs"
    }
  ],
  "schemaVersion": 39,
  "tags": ["omni-quantum", "logs", "loki", "observability", "system-6"],
  "templating": {
    "list": [
      {
        "current": { "selected": true, "text": "All", "value": "$__all" },
        "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
        "definition": "label_values({omni_tier=~\".+\"}, omni_component)",
        "hide": 0,
        "includeAll": true,
        "label": "Service",
        "multi": true,
        "name": "service",
        "options": [],
        "query": "label_values({omni_tier=~\".+\"}, omni_component)",
        "refresh": 2,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "type": "query"
      },
      {
        "current": { "selected": true, "text": ["All"], "value": ["$__all"] },
        "hide": 0,
        "includeAll": true,
        "label": "Level",
        "multi": true,
        "name": "level",
        "options": [
          { "selected": false, "text": "critical", "value": "critical" },
          { "selected": false, "text": "error", "value": "error" },
          { "selected": false, "text": "warning", "value": "warning" },
          { "selected": false, "text": "info", "value": "info" },
          { "selected": false, "text": "debug", "value": "debug" }
        ],
        "query": "critical,error,warning,info,debug",
        "skipUrlSync": false,
        "type": "custom"
      },
      {
        "current": { "selected": false, "text": "", "value": "" },
        "hide": 0,
        "label": "Search Text",
        "name": "search_filter",
        "options": [{ "selected": true, "text": "", "value": "" }],
        "query": "",
        "skipUrlSync": false,
        "type": "textbox"
      },
      {
        "current": { "selected": false, "text": "", "value": "" },
        "hide": 0,
        "label": "Trace ID",
        "name": "trace_id",
        "options": [{ "selected": true, "text": "", "value": "" }],
        "query": "",
        "skipUrlSync": false,
        "type": "textbox"
      },
      {
        "current": { "selected": false, "text": "Loki", "value": "loki" },
        "hide": 2,
        "includeAll": false,
        "label": "Datasource",
        "multi": false,
        "name": "DS_LOKI",
        "options": [],
        "query": "loki",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "type": "datasource"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "timezone": "browser",
  "title": "Omni Quantum — Log Explorer",
  "uid": "omni-log-explorer",
  "version": 1
}
