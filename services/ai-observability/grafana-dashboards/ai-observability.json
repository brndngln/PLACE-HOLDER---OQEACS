{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "Prometheus datasource for Langfuse metrics",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    },
    {
      "name": "DS_LANGFUSE",
      "label": "Langfuse API",
      "description": "Langfuse JSON API datasource",
      "type": "datasource",
      "pluginId": "yesoreyeram-infinity-datasource",
      "pluginName": "Infinity"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" },
    { "type": "panel", "id": "barchart", "name": "Bar chart", "version": "" },
    { "type": "panel", "id": "histogram", "name": "Histogram", "version": "" },
    { "type": "panel", "id": "table", "name": "Table", "version": "" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" },
    { "type": "panel", "id": "gauge", "name": "Gauge", "version": "" }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "description": "Omni Quantum Elite — AI Observability: LLM costs, token usage, quality scores, agent comparison, and cache analytics",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [
    {
      "asDropdown": false,
      "icon": "external link",
      "includeVars": true,
      "keepTime": true,
      "tags": ["omni-quantum"],
      "targetBlank": true,
      "title": "Log Explorer",
      "type": "dashboards"
    }
  ],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 200,
      "title": "Cost & Token Overview",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Total LLM API cost today",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "red", "value": 200 }
            ]
          },
          "unit": "currencyUSD"
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "id": 1,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum(increase(langfuse_llm_cost_usd_total[$__range]))",
          "legendFormat": "Total Cost",
          "refId": "A"
        }
      ],
      "title": "Total LLM Cost (Period)",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Total tokens consumed across all models",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1000000 },
              { "color": "red", "value": 10000000 }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "id": 2,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum(increase(langfuse_llm_tokens_total[$__range]))",
          "legendFormat": "Total Tokens",
          "refId": "A"
        }
      ],
      "title": "Total Tokens (Period)",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Total number of LLM API calls",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "blue", "value": null },
              { "color": "green", "value": 100 }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "id": 3,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum(increase(langfuse_llm_calls_total[$__range]))",
          "legendFormat": "LLM Calls",
          "refId": "A"
        }
      ],
      "title": "LLM Calls (Period)",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Average cost per LLM call",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.1 },
              { "color": "red", "value": 0.5 }
            ]
          },
          "unit": "currencyUSD"
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "id": 4,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum(increase(langfuse_llm_cost_usd_total[$__range])) / sum(increase(langfuse_llm_calls_total[$__range]))",
          "legendFormat": "Avg Cost/Call",
          "refId": "A"
        }
      ],
      "title": "Avg Cost per Call",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "LLM API cost over time, broken down by model. Track spending trends and identify cost spikes.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisColorMode": "text",
            "axisLabel": "Cost (USD)",
            "drawStyle": "line",
            "fillOpacity": 20,
            "gradientMode": "scheme",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "stacking": { "group": "A", "mode": "normal" }
          },
          "unit": "currencyUSD"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 5 },
      "id": 10,
      "options": {
        "legend": { "calcs": ["sum", "mean"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by (model) (increase(langfuse_llm_cost_usd_total{model=~\"$model\"}[1h]))",
          "legendFormat": "{{model}}",
          "refId": "A"
        }
      ],
      "title": "Panel 1: LLM Cost Over Time",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Token usage stacked by type (prompt vs completion) and model.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisLabel": "Tokens",
            "fillOpacity": 80,
            "lineWidth": 1,
            "stacking": { "group": "A", "mode": "normal" }
          },
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": { "id": "byRegexp", "options": ".*prompt.*" },
            "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byRegexp", "options": ".*completion.*" },
            "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 5 },
      "id": 20,
      "options": {
        "barRadius": 0,
        "barWidth": 0.9,
        "groupWidth": 0.7,
        "legend": { "calcs": ["sum"], "displayMode": "table", "placement": "bottom" },
        "orientation": "auto",
        "showValue": "never",
        "stacking": "normal",
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by (model) (increase(langfuse_llm_prompt_tokens_total{model=~\"$model\"}[1h]))",
          "legendFormat": "{{model}} prompt",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by (model) (increase(langfuse_llm_completion_tokens_total{model=~\"$model\"}[1h]))",
          "legendFormat": "{{model}} completion",
          "refId": "B"
        }
      ],
      "title": "Panel 2: Token Usage (Prompt vs Completion)",
      "type": "barchart"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 13 },
      "id": 201,
      "title": "Latency & Quality",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "LLM call latency distribution with p50, p95, and p99 percentiles by model.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "fillOpacity": 30,
            "gradientMode": "scheme",
            "lineWidth": 1
          },
          "unit": "s"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
      "id": 30,
      "options": {
        "bucketOffset": 0,
        "combine": false,
        "fillOpacity": 30,
        "gradientMode": "scheme",
        "legend": { "calcs": [], "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "histogram_quantile(0.50, sum by (le, model) (rate(langfuse_llm_duration_seconds_bucket{model=~\"$model\"}[$__rate_interval])))",
          "legendFormat": "p50 {{model}}",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "histogram_quantile(0.95, sum by (le, model) (rate(langfuse_llm_duration_seconds_bucket{model=~\"$model\"}[$__rate_interval])))",
          "legendFormat": "p95 {{model}}",
          "refId": "B"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "histogram_quantile(0.99, sum by (le, model) (rate(langfuse_llm_duration_seconds_bucket{model=~\"$model\"}[$__rate_interval])))",
          "legendFormat": "p99 {{model}}",
          "refId": "C"
        }
      ],
      "title": "Panel 3: Latency Distribution (p50/p95/p99)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Average code review quality scores over time with trend line. Tracks quality improvement or degradation.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisColorMode": "text",
            "axisLabel": "Score (1-10)",
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "scheme",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "thresholdsStyle": { "mode": "line+area" }
          },
          "min": 0,
          "max": 10,
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "green", "value": 7 }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
      "id": 40,
      "options": {
        "legend": { "calcs": ["mean", "lastNotNull"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "avg(langfuse_score_value{score_name=\"overall_score\"})",
          "legendFormat": "Overall Score",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "avg(langfuse_score_value{score_name=\"correctness\"})",
          "legendFormat": "Correctness",
          "refId": "B"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "avg(langfuse_score_value{score_name=\"security\"})",
          "legendFormat": "Security",
          "refId": "C"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "avg(langfuse_score_value{score_name=\"performance\"})",
          "legendFormat": "Performance",
          "refId": "D"
        }
      ],
      "title": "Panel 4: Quality Scores Over Time",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 22 },
      "id": 202,
      "title": "Agent Performance & Cost Analysis",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Head-to-head comparison: OpenHands vs SWE-Agent across key metrics — success rate, quality, cost, and time.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": {
            "align": "center",
            "cellOptions": { "type": "auto" },
            "inspect": false
          },
          "thresholds": {
            "steps": [{ "color": "text", "value": null }]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Success Rate" },
            "properties": [
              { "id": "unit", "value": "percentunit" },
              { "id": "custom.cellOptions", "value": { "mode": "gradient", "type": "gauge" } },
              { "id": "color", "value": { "mode": "continuous-GrYlRd" } },
              { "id": "min", "value": 0 },
              { "id": "max", "value": 1 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Avg Quality" },
            "properties": [
              { "id": "custom.cellOptions", "value": { "mode": "gradient", "type": "gauge" } },
              { "id": "color", "value": { "mode": "continuous-GrYlRd" } },
              { "id": "min", "value": 0 },
              { "id": "max", "value": 10 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Avg Cost/Task" },
            "properties": [
              { "id": "unit", "value": "currencyUSD" },
              { "id": "color", "value": { "mode": "continuous-RdYlGr" } },
              { "id": "custom.cellOptions", "value": { "mode": "gradient", "type": "gauge" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Avg Time/Task" },
            "properties": [
              { "id": "unit", "value": "s" },
              { "id": "color", "value": { "mode": "continuous-RdYlGr" } },
              { "id": "custom.cellOptions", "value": { "mode": "gradient", "type": "gauge" } }
            ]
          }
        ]
      },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 23 },
      "id": 50,
      "options": {
        "cellHeight": "md",
        "footer": { "countRows": false, "fields": "", "reducer": [], "show": false },
        "showHeader": true
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "langfuse_agent_success_rate{agent=~\"openhands|swe-agent\"}",
          "format": "table",
          "legendFormat": "{{agent}}",
          "refId": "A",
          "instant": true
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "avg by (agent) (langfuse_score_value{agent=~\"openhands|swe-agent\", score_name=\"overall_score\"})",
          "format": "table",
          "legendFormat": "{{agent}}",
          "refId": "B",
          "instant": true
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "avg by (agent) (langfuse_agent_task_cost_usd{agent=~\"openhands|swe-agent\"})",
          "format": "table",
          "legendFormat": "{{agent}}",
          "refId": "C",
          "instant": true
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "avg by (agent) (langfuse_agent_task_duration_seconds{agent=~\"openhands|swe-agent\"})",
          "format": "table",
          "legendFormat": "{{agent}}",
          "refId": "D",
          "instant": true
        }
      ],
      "title": "Panel 5: Agent Comparison — OpenHands vs SWE-Agent",
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true },
            "renameByName": {
              "agent": "Agent",
              "Value #A": "Success Rate",
              "Value #B": "Avg Quality",
              "Value #C": "Avg Cost/Task",
              "Value #D": "Avg Time/Task"
            }
          }
        }
      ],
      "type": "table"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Most expensive tasks sorted by total LLM cost. Click a trace ID to open in Langfuse.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "auto" },
            "inspect": false
          },
          "thresholds": {
            "steps": [{ "color": "text", "value": null }]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Cost (USD)" },
            "properties": [
              { "id": "unit", "value": "currencyUSD" },
              { "id": "custom.cellOptions", "value": { "mode": "gradient", "type": "gauge" } },
              { "id": "color", "value": { "mode": "continuous-reds" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Tokens" },
            "properties": [{ "id": "unit", "value": "short" }]
          },
          {
            "matcher": { "id": "byName", "options": "Duration" },
            "properties": [{ "id": "unit", "value": "s" }]
          },
          {
            "matcher": { "id": "byName", "options": "Trace ID" },
            "properties": [
              {
                "id": "links",
                "value": [
                  {
                    "targetBlank": true,
                    "title": "Open in Langfuse",
                    "url": "http://omni-langfuse:3000/trace/${__value.text}"
                  }
                ]
              }
            ]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 29 },
      "id": 60,
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": true },
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "Cost (USD)" }]
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "topk(20, langfuse_trace_cost_usd)",
          "format": "table",
          "legendFormat": "",
          "refId": "A",
          "instant": true
        }
      ],
      "title": "Panel 6: Most Expensive Tasks",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "__name__": true, "instance": true, "job": true },
            "renameByName": {
              "trace_id": "Trace ID",
              "trace_name": "Task Name",
              "model": "Model",
              "Value": "Cost (USD)"
            }
          }
        }
      ],
      "type": "table"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Percentage of LLM calls that resulted in errors, broken down by model. Track API reliability.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisLabel": "Error Rate",
            "drawStyle": "line",
            "fillOpacity": 20,
            "gradientMode": "scheme",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "thresholdsStyle": { "mode": "line" }
          },
          "min": 0,
          "max": 1,
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.01 },
              { "color": "red", "value": 0.05 }
            ]
          },
          "unit": "percentunit"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 29 },
      "id": 70,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by (model) (rate(langfuse_llm_errors_total{model=~\"$model\"}[$__rate_interval])) / sum by (model) (rate(langfuse_llm_calls_total{model=~\"$model\"}[$__rate_interval]))",
          "legendFormat": "{{model}}",
          "refId": "A"
        }
      ],
      "title": "Panel 7: LLM Error Rate by Model",
      "type": "timeseries"
    },
    {
